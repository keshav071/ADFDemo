{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "KeshavFactory"
		},
		"PracticeAzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'PracticeAzureBlobStorage1'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"SqlServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer1'"
		},
		"SqlServer_dynmic_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer_dynmic'"
		},
		"SqlServer_dynmic_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SqlServer_dynmic'"
		},
		"SqlServer1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "MSI\\kesha"
		},
		"SqlServer_dynmic_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "MSI\\kesha"
		},
		"fordynmic_properties_pipeline_parameters_dbname": {
			"type": "string",
			"defaultValue": "emp"
		},
		"fordynmic_properties_pipeline_parameters_iptable1": {
			"type": "string",
			"defaultValue": "customer"
		},
		"fordynmic_properties_pipeline_parameters_optable2": {
			"type": "string",
			"defaultValue": "customer"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "KeshavFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"PracticeAzureBlobStorage1_connectionString": {
						"value": "[parameters('PracticeAzureBlobStorage1_connectionString')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"SqlServer1_password": {
						"value": "[parameters('SqlServer1_password')]"
					},
					"SqlServer1_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "KeshavFactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SqlServer_dynmic_connectionString": {
						"value": "[parameters('SqlServer_dynmic_connectionString')]"
					},
					"SqlServer_dynmic_password": {
						"value": "[parameters('SqlServer_dynmic_password')]"
					},
					"SqlServer_dynmic_properties_typeProperties_userName": {
						"value": "[parameters('SqlServer_dynmic_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'KeshavFactory_ArmTemplate_0')]"
			]
		},
		{
			"name": "KeshavFactory_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"fordynmic_properties_pipeline_parameters_dbname": {
						"value": "[parameters('fordynmic_properties_pipeline_parameters_dbname')]"
					},
					"fordynmic_properties_pipeline_parameters_iptable1": {
						"value": "[parameters('fordynmic_properties_pipeline_parameters_iptable1')]"
					},
					"fordynmic_properties_pipeline_parameters_optable2": {
						"value": "[parameters('fordynmic_properties_pipeline_parameters_optable2')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'KeshavFactory_ArmTemplate_1')]"
			]
		}
	]
}